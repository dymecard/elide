{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "types": []
  },
  "exclude": [
    "bazel-*",
    "dist"
  ],
  "bazelOptions": {
    "workspaceName": "gust"
  }
}